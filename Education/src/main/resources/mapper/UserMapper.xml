<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.work1.mapper.UserMapper">
    <select id="findByUsernameAndPassword" resultType="org.example.work1.entity.User">
        SELECT * FROM ts WHERE username = #{username} AND password = #{password}
    </select>
</mapper> 